{
  "name": "H1",
  "title": "H1",
  "description": "H1: STM32  MCU综合开发平台",
  "url": "http://www.st.com/web/en/catalog/mmc/FM141/SC1169?sc=stm32",
  "homepage": "https://github.com/OS-Q/H1",
  "license": " ",
  "engines": {
    "platformio": "<5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/H1.git"
  },
  "version": "1.0.0",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json"
  ],
  "frameworks": {
    "mbed": {
      "package": "framework-N2",
      "script": "builder/frameworks/mbed.py"
    },
    "N4": {
      "package": "framework-N4",
      "script": "builder/frameworks/libopencm3/libopencm3.py"
    },
    "arduino": {
      "package": "framework-stm32arduino",
      "script": "builder/frameworks/arduino.py"
    },
    "stm32cube": {
      "package": "framework-stcube",
      "script": "builder/frameworks/stm32cube.py"
    }
  },
  "packages": {
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "version": ">=1.60301.0,<1.80000.0"
    },
    "framework-N2": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N2.git"
    },
    "framework-N4": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N4.git"
    },
    "framework-stm32arduino": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N5.git"
    },
    "framework-stm32maple": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N3.git"
    },
    "framework-arduinostm32mxchip": {
      "type": "framework",
      "optional": true,
      "version": "~1.3.0"
    },
    "framework-stcube": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N1.git"
    },
    "tool-stm32duino": {
      "type": "uploader",
      "optional": true,
      "version": "~1.0.1"
    },
    "tool-openocd": {
      "type": "uploader",
      "optional": true,
      "version": "~2.1000.0"
    },
    "tool-jlink": {
      "type": "uploader",
      "optional": true,
      "version": "^1.63208.0"
    },
    "tool-dfuutil": {
      "type": "uploader",
      "optional": true,
      "version": "~1.9.190708"
    }
  }
}
